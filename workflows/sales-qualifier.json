{
    "name": "WhatsApp Sales Qualifier Agent",
    "nodes": [
        {
            "parameters": {
                "httpMethod": "POST",
                "path": "whatsapp-webhook",
                "options": {}
            },
            "id": "1",
            "name": "WhatsApp Trigger",
            "type": "n8n-nodes-base.webhook",
            "typeVersion": 1,
            "position": [
                250,
                300
            ]
        },
        {
            "parameters": {
                "model": "gpt-4o",
                "systemMessage": "You are a professional sales qualifier for a premium tech agency. Your goal is to identify if the user needs mobile development, web development, or AI automation. Be helpful but efficient.",
