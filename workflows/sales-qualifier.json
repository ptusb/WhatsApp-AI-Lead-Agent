                        }
                    ]
                }
            },
            "id": "3",
            "name": "Is Qualified?",
            "type": "n8n-nodes-base.if",
            "typeVersion": 1,
            "position": [
                650,
                300
            ]
        }
    ],
    "connections": {
        "WhatsApp Trigger": {
            "main": [
                [
                    {
                        "node": "Qualify Intent",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Qualify Intent": {
            "main": [
                [
                    {
                        "node": "Is Qualified?",
